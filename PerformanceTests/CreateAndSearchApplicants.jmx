        <JSR223Sampler guiclass="TestBeanGUI" testclass="JSR223Sampler" testname="Сгенерировать заявителя" enabled="true">
          <stringProp name="cacheKey">true</stringProp>
          <stringProp name="filename"></stringProp>
          <stringProp name="parameters"></stringProp>
          <stringProp name="script">def sampleGenerator = { String alphabet, int n -&gt;
  new Random().with {
    (1..n).collect { alphabet[ nextInt( alphabet.length() ) ] }.join()
  }
}

def numberString = {int count -&gt; 
	sampleGenerator((&apos;0&apos;..&apos;9&apos;).join(), count)
}

def cyrillicString = {int count -&gt; 
	sampleGenerator((&apos;А&apos;..&apos;Я&apos;).join(), count)
}

def namePrefix = props.getProperty(&quot;NAME_PREFIX&quot;)
def randomSnils = &quot;${numberString(3)}-${numberString(3)}-${numberString(3)} ${numberString(2)}&quot;
def firstName = &quot;${namePrefix}-${cyrillicString(5)}&quot;

log.info(&quot;Applicant first name: {}&quot;, firstName)

vars.put(&quot;SNILS&quot;, randomSnils)
vars.put(&quot;FIRST_NAME&quot;, firstName)
vars.put(&quot;PATRONYMIC&quot;, cyrillicString(8))
vars.put(&quot;LAST_NAME&quot;, cyrillicString(10))
</stringProp>
